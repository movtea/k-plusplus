{
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: g++ build active file",
			"command": "g++",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"${file}",
				"${workspaceFolder}/build/sqlite3.o",
				"${workspaceFolder}/src/getFileFromDir/getFileFromDir.cpp",
				"${workspaceFolder}/src/nsrlRepository/nsrlRepository.cpp",
				"${workspaceFolder}/src/OutputDB/outputDB.cpp",
				"${workspaceFolder}/src/calculateShaHash/calculateShaHash.cpp",
				"-o",
				"${workspaceFolder}/build//${fileBasenameNoExtension}",
				"-L${workspaceFolder}/lib/cryptopp/",
				"-l:libcryptopp.a"
			],
			"options": {
				"cwd": "${workspaceFolder}/build/"
			},
			"problemMatcher": ["$gcc"],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "Task generated by Debugger."
		}
	],
	"version": "2.0.0"
}
